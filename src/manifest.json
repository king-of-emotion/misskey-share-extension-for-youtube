{
  "name": "misskey-share-button-for-youtube",
  "version": "1.2.0",
  "manifest_version": 3,
  "description": "youtubeのシェアメニューにmisskeyへの共有ボタンを追加します",
  "action": {
    "matches": ["https://www.youtube.com/*"],
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": [
      "content.js",
      "common.js"
    ],
    "css": ["on_youtube.css"]
    },
    {
      "matches": ["https://studio.youtube.com/*"],
      "js": [
        "content_on_youtube_studio.js",
        "common.js"
      ],
      "css": ["on_youtube.css"]
      }],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}