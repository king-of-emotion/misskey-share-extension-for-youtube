{
  "name": "misskey-share-for-youtube",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "you can share youtube contents on misskey",
  "action": {
    "matches": ["https://www.youtube.com/*"],
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": [
      "content.js"
    ],
    "css": ["on_youtube.css"]
    }],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}